{
  "files.associations": {
    "*.json": "jsonc"
  },

  "typescript.tsdk": "node_modules\\typescript\\lib",

  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "icon": "bug"
    },
    "HACK": {
      "icon": "tools"
    },
    "FIXME": {
      "icon": "flame"
    },
    "XXX": {
      "icon": "x"
    },
    "[ ]": {
      "icon": "issue-draft"
    },
    "[x]": {
      "icon": "issue-closed"
    },
    "p1": {
      "icon": "alert-fill",
      "iconColour": "orange"
    },
    "y1": {
      "icon": "bell",
      "iconColour": "orange",
      "background": "orange"
    },
    "y2": {
      "icon": "bell",
      "iconColour": "green"
    }
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "p1",
    "p1.5",
    "p2",
    "y1",
    "y2"
  ],
  "search.exclude": {
    "./dist/**/*": true
  },
  "spellchecker.ignoreWordsList": [
    "dbNamesOf",
    "knex",
    "angular",
    "angular's",
    "autoincrement",
    "chromedriver",
    "circleci",
    "codecov",
    "codelyzer",
    "copyfiles",
    "devkit",
    "expressjwt",
    "Honig",
    "lcov",
    "ng-packagr",
    "Noam",
    "Nuxt",
    "openapi",
    "packagr",
    "paramtypes",
    "postgres",
    "postgresql",
    "Prefilter",
    "Quickstart",
    "remult",
    "remult-nextjs-todo",
    "remult's",
    "respawn",
    "scroller",
    "styleext",
    "Supabase",
    "tldr",
    "Todos",
    "typedoc",
    "unchecks",
    "unopinionated",
    "websql",
    "backend",
    "const",
    "wrapIdentifier",
    "allowapicrud"
  ],
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "javascript.preferences.autoImportFileExcludePatterns": ["node:test"]
}
